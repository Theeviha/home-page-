}
.nav ul li a {
  text-decoration: none;
  color:#1f166a;
  text-transform: capitalize;
}


.nav ul li::after{
  content: '';
  height: 2px;
  width: 0; /*100% */
  background: #2d13a0;
  position: absolute;
  left: 0;
  bottom: -8px;
  transition: 0.5s;
  
}


.nav ul li:hover::after {
  width: 100%;
}


.content {
  width: 100%;
  position: absolute;
  top: 90%;
 margin-top: 10PX;
